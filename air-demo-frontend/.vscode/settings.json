{
	"files.associations": {
		"*.css": "tailwindcss"
	},
	"editor.quickSuggestions": {
		"strings": "on"
	},
	"tailwindCSS.classAttributes": ["class", "ui"],
	"tailwindCSS.experimental.classRegex": [["ui:\\s*{([^)]*)\\s*}", "(?:'|\"|`)([^']*)(?:'|\"|`)"]],
	"cSpell.words": [
		"boxlayout",
		"Gujrant",
		"Ioane",
		"itemover",
		"lucide",
		"Nuxt",
		"Pintone",
		"reka",
		"timechanged",
		"zoomable"
	]
}
